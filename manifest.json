{

    "manifest_version": 2,
    "name": "Vulcan.js",
    "version": "1.0",

    "browser_specific_settings": {
      "gecko": {
        "id": "vulcan.js@natangrygiel.pl"
      }
    },
    
  
    "description": "Change the look and feel of your Vulcan gradebook with this extension.", 
    "homepage_url": "https://natangrygiel.pl/vulcanjs",
  
    "icons": {
        "16": "logo/logo16.png",
        "32": "logo/logo32.png",
      "48": "logo/logo48.png",
        "64": "logo/logo64.png",
      "128": "logo/logo128.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.vulcan.net.pl/*"],
        "js": ["theme/folly/vulcan.js"]
      }
    ]
  
  }